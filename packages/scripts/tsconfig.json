{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./lib",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "DOM"
    ],
    "resolveJsonModule": true
  },
  "include": [
    "./src/**/*.ts",
    "index copy.ts"
  ]
}